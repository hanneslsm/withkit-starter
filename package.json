{
	"name": "withkit",
	"version": "0.1.0",
	"main": "index.js",
	"scripts": {
		"start": "wp-scripts start --config webpack.config.js --env development ",
		"build": "wp-scripts build --config webpack.config.js --env production",
		"lint:css": "wp-scripts lint-style 'src/scss/*'",
		"lint:css:fix": "wp-scripts lint-style 'src/scss/*' --fix",
		"lint:js": "wp-scripts lint-js 'src/js/*'",
		"format": "wp-scripts format 'src/**/*.{js,jsx,json,ts,tsx,css,scss,php}' '!src/fonts/**'",
		"zip": "wp-scripts plugin-zip",
		"packages-update": "wp-scripts packages-update"
	},
	"files": [
		"/*",
		"!/withkit",
		"!/withkit.zip",
		"!/src",
		"!.vscode",
		"!.editorconfig",
		"!.gitignore",
		"!package.json",
		"!webpack.config.js"
	],
	"author": "hanneslsm",
	"license": "GPL-2.0",
	"description": "",
	"devDependencies": {
		"@wordpress/scripts": "^30.19.0",
		"browser-sync-webpack-plugin": "^2.4.0",
		"copy-webpack-plugin": "^12.0.2",
		"glob": "^11.0.0",
		"image-minimizer-webpack-plugin": "^4.1.1",
		"imagemin": "^9.0.0",
		"imagemin-webpack-plugin": "^2.4.2",
		"sharp": "^0.33.5",
		"webpack-remove-empty-scripts": "^1.0.4"
	},
	"dependencies": {
		"gsap": "^3.13.0"
	}
}
